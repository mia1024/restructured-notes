{
  "name": "restructured-notes",
  "productName": "Restructured Notes",
  "version": "0.0.1",
  "license": "GPL-3.0",
  "scripts": {
    "dev": "electron-webpack dev",
    "compile": "electron-webpack",
    "dist": "yarn compile && electron-builder",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null",
    "test": "ELECTRON_RUN_AS_NODE=1 electron ./node_modules/.bin/mochapack --webpack-config node_modules/electron-webpack/webpack.renderer.config.js --require ./test/setup.js --reporter nyan ./test/**/*.spec.ts ./test/*.spec.ts",
    "test:nonyan": "ELECTRON_RUN_AS_NODE=1 electron ./node_modules/.bin/mochapack --webpack-config node_modules/electron-webpack/webpack.renderer.config.js --require ./test/setup.js ./test/**/*.spec.ts ./test/*.spec.ts",
    "test:noelectron": "mochapack --webpack-config node_modules/electron-webpack/webpack.renderer.config.js --require ./test/setup.js --reporter nyan ./test/**/*.spec.ts ./test/*.spec.ts",
    "test:noelectron:nonyan": "mochapack --webpack-config node_modules/electron-webpack/webpack.renderer.config.js --require ./test/setup.js ./test/**/*.spec.ts ./test/*.spec.ts",
    "compile-dependencies": "electron-builder install-app-deps"
  },
  "dependencies": {
    "nodegit": "^0.26.5",
    "source-map-support": "^0.5.16",
    "sqlite3": "^4.2.0"
  },
  "devDependencies": {
    "@mdi/font": "^5.3.45",
    "@nuxtjs/vuetify": "^1.11.2",
    "@types/expect": "^24.3.0",
    "@types/mocha": "^7.0.2",
    "@types/node": "12",
    "@types/nodegit": "^0.26.5",
    "@types/sqlite3": "^3.1.6",
    "@types/uuid": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "@vue/test-utils": "^1.0.3",
    "csstype": "^2.6.10",
    "deepmerge": "^4.2.2",
    "electron": "^8.3",
    "electron-builder": "^22.4.1",
    "electron-mocha": "^8.2.2",
    "electron-rebuild": "^1.11.0",
    "electron-webpack": "^2.8.2",
    "electron-webpack-ts": "^4.0.1",
    "electron-webpack-vue": "^2.4.0",
    "eslint": "^7.3.1",
    "eslint-plugin-html": "^6.0.2",
    "eslint-plugin-vue": "^6.2.2",
    "expect": "^26.0.1",
    "fabric": "^3.6.3",
    "fibers": "^5.0.0",
    "jquery": "^3.5.1",
    "jsdom": "^16.2.2",
    "jsdom-global": "^3.0.2",
    "mathjax": "^3.0.5",
    "mocha": "^8.0.1",
    "mochapack": "^2.0.1",
    "node-sass": "^4.14.1",
    "openpgp": "^4.10.4",
    "raw-loader": "^4.0.1",
    "sass": "^1.26.8",
    "sass-loader": "^8.0.2",
    "tesseract.js": "^2.1.1",
    "ts-mocha": "^7.0.0",
    "typescript": "^4.0.0-beta",
    "uuid": "^8.2.0",
    "vue": "^2.6.11",
    "vue-property-decorator": "^8.5.0",
    "vue-test": "^1.0.0",
    "vuex": "^3.4.0",
    "webpack": "~4.42.1",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2",
    "yaml": "^1.10.0"
  },
  "description": "A (hopefully) better note taking app",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mia1024/restructured-notes.git"
  },
  "keywords": [
    "note",
    "notes"
  ],
  "author": "Mia Celeste",
  "bugs": {
    "url": "https://github.com/mia1024/restructured-notes/issues"
  },
  "homepage": "https://github.com/mia1024/restructured-notes#readme"
}
